{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 10
      }
    }
  ],
 "routes": [
  {
    "handle": "filesystem"
  },
  {
    "src": "/(.*)",
    "dest": "/index.html"
  }
],
  "env": {
    "NODE_ENV": "production"
  }
}
